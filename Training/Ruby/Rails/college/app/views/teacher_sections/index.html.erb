<p id="notice"><%= notice %></p>

<h1>Teacher Sections</h1>

<table>
  <thead>
    <tr>
      <th>Teacher</th>
      <th>Section</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teacher_sections.each do |teacher_section| %>
      <tr>
        <td><%= teacher_section.teacher %></td>
        <td><%= teacher_section.section %></td>
        <td><%= link_to 'Show', teacher_section %></td>
        <td><%= link_to 'Edit', edit_teacher_section_path(teacher_section) %></td>
        <td><%= link_to 'Destroy', teacher_section, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Teacher Section', new_teacher_section_path %>
